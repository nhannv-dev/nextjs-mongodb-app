{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "next/core-web-vitals", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "@next/next/no-img-element": "off",
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto",
        "printWidth": 100,
        "semi": false
      }
    ]
  }
}
